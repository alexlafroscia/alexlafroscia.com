{
  "name": "my-app",
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --check --plugin-search-dir=. . && eslint --ignore-path .prettierignore .",
    "format": "prettier --write --plugin-search-dir=. .",
    "test:unit": "vitest",
    "test:acceptance": "yarn build && PLAYWRIGHT_BASE_URL=http://localhost:4173 start-server-and-test preview http://localhost:4173 test:acceptance:run",
    "test:acceptance:run": "playwright test"
  },
  "devDependencies": {
    "@mapbox/rehype-prism": "^0.8.0",
    "@playwright/test": "^1.28.0",
    "@rollup/plugin-yaml": "^3.1.0",
    "@sveltejs/adapter-vercel": "^1.0.0-next.81",
    "@sveltejs/kit": "^1.0.0-next.552",
    "@types/unist": "^2.0.6",
    "@types/xml": "^1.0.6",
    "@typescript-eslint/eslint-plugin": "^5.43.0",
    "@typescript-eslint/parser": "^5.43.0",
    "autoprefixer": "^10.4.0",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "feed": "^4.2.2",
    "fuse.js": "^6.4.6",
    "hastscript": "^7.0.2",
    "jsdom": "^19.0.0",
    "mdsvex": "^0.10.5",
    "postcss": "^8.4.16",
    "prettier": "^2.7.1",
    "prettier-plugin-svelte": "^2.8.1",
    "rehype": "^11.0.0",
    "rehype-autolink-headings": "^6.1.1",
    "rehype-slug": "^5.0.1",
    "start-server-and-test": "^1.14.0",
    "svelte": "^3.49.0",
    "svelte-check": "^2.9.2",
    "svelte-preprocess": "^4.10.7",
    "tailwindcss": "^3.1.8",
    "tslib": "^2.4.1",
    "typescript": "^4.9.3",
    "vite": "^3.2.4",
    "vitest": "^0.25.2",
    "web-vitals": "^3.1.0"
  },
  "packageManager": "yarn@3.3.0",
  "volta": {
    "node": "16.15.0",
    "yarn": "1.22.17"
  }
}
