{
  "name": "alexlafroscia.com",
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --check --plugin prettier-plugin-svelte . && eslint --ignore-path .prettierignore .",
    "format": "prettier --write --plugin prettier-plugin-svelte .",
    "test": "run-p --print-label 'test:*'",
    "test:unit": "vitest run",
    "test:acceptance": "yarn build && PLAYWRIGHT_BASE_URL=http://localhost:4173 start-server-and-test 'yarn preview' http://localhost:4173 'yarn test:acceptance:run'",
    "test:acceptance:run": "playwright test",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.0.2",
    "@mapbox/rehype-prism": "^0.8.0",
    "@playwright/test": "^1.28.0",
    "@rollup/plugin-yaml": "^4.0.1",
    "@storybook/addon-essentials": "^8.0.10",
    "@storybook/addon-interactions": "^8.0.10",
    "@storybook/addon-links": "^8.0.10",
    "@storybook/blocks": "^8.0.10",
    "@storybook/svelte": "^8.0.10",
    "@storybook/sveltekit": "^8.0.10",
    "@sveltejs/adapter-vercel": "^5.3.0",
    "@sveltejs/kit": "^2.5.7",
    "@sveltejs/vite-plugin-svelte": "^3.1.0",
    "@types/unist": "^2.0.6",
    "@types/xml": "^1.0.11",
    "@typescript-eslint/eslint-plugin": "^5.43.0",
    "@typescript-eslint/parser": "^5.43.0",
    "autoprefixer": "^10.4.0",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-storybook": "^0.6.13",
    "eslint-plugin-svelte3": "^4.0.0",
    "feed": "^4.2.2",
    "hastscript": "^7.0.2",
    "jsdom": "^19.0.0",
    "mdsvex": "^0.10.5",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.16",
    "prettier": "^3.2.5",
    "prettier-plugin-svelte": "^3.2.3",
    "rehype": "^11.0.0",
    "rehype-autolink-headings": "^6.1.1",
    "rehype-slug": "^5.0.1",
    "start-server-and-test": "^1.14.0",
    "storybook": "^8.0.10",
    "svelte": "^4.2.0",
    "svelte-check": "^3.5.2",
    "svelte-preprocess": "^5.1.4",
    "tailwindcss": "^3.1.8",
    "tslib": "^2.4.1",
    "typescript": "^5.2.2",
    "vite": "^5.2.11",
    "vite-tsconfig-paths": "^4.0.3",
    "vitest": "^1.6.0",
    "web-vitals": "^3.1.0"
  },
  "packageManager": "yarn@3.3.0",
  "volta": {
    "node": "18.16.0",
    "yarn": "1.22.17"
  }
}
